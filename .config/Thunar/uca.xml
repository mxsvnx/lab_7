<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>Terminal</icon>
	<name>Открыть в терминале</name>
	<submenu></submenu>
	<unique-id>1749559447760404-1</unique-id>
	<command>exo-open --working-directory %f --launch TerminalEmulator</command>
	<description>Открыть терминал в выделенной папке</description>
	<range></range>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>folder</icon>
	<name>Открыть как root</name>
	<submenu></submenu>
	<unique-id>1749559447760513-2</unique-id>
	<command>pkexec thunar %F</command>
	<description>Открыть папку с привилегиями root</description>
	<range></range>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>emblem-symbolic-link</icon>
	<name>Создать ссылку</name>
	<submenu></submenu>
	<unique-id>1749559447760622-3</unique-id>
	<command>ln -s %f  &apos;Link to %n&apos;</command>
	<description>Создать символьную ссылки для выделенных объектов</description>
	<range></range>
	<patterns>*</patterns>
	<directories/>
	<other-files/>
</action>
<action>
	<icon>application-certificate</icon>
	<name>Проверить</name>
	<submenu></submenu>
	<unique-id>1749559447760659-4</unique-id>
	<command>mint-iso-verify %f</command>
	<description>Проверка подлинности и целостности образа</description>
	<range></range>
	<patterns>*.iso;*.ISO</patterns>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>gtk-find</icon>
	<name>Поиск</name>
	<submenu></submenu>
	<unique-id>1749559447760784-5</unique-id>
	<command>catfish %f</command>
	<description>Поиск файлов</description>
	<range></range>
	<patterns>*</patterns>
	<directories/>
</action>
</actions>
